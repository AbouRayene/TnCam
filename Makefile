		DS_OPTS="-O2 -DOS_LINUX -DTUXBOX -DMIPSEL -DCS_CONFDIR='\"/var/tuxbox/config\"' -static-libgcc -D'CS_SVN_VERSION="'"$(shell svnversion -n . | sed 's/.$$//' )"'"'" \
		DS_CFLAGS="-c" \
		DS_LDFLAGS="" \
		DS_ARFLAGS="-rvsl" \
		DS_CC=mipsel-linux-gcc \
		DS_AR=mipsel-linux-ar \
		DS_LD=mipsel-linux-ld \
		DS_RL=mipsel-linux-ranlib \
		DS_ST=mipsel-linux-strip

